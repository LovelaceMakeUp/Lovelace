<title>Cadastro</title>

<!-- start main -->
<div class="main_bg">
<div class="wrap">
<div class="main">
  
  <!-- end registration -->
  
  <div class="login_left_unique">
    <h3>Login</h3>
    <div class="registration_left">
    <!--<a href="#"><div class="reg_fb"><img src="images/facebook.png" alt=""><i>register using Facebook</i><div class="clear"></div></div></a>-->
    

     <div class="registration_form">
     <!-- Form -->
     <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <form id="registration_form">
        <div>
          <label>
            <%= f.text_field :email, :placeholder => 'Email:', :class=>"form-control" %>
          </label>
        </div>
        <div>
          <label>
            <%= f.text_field :password, :placeholder => 'Senha:', :type=>"password" %>
          </label>
        </div>            
        <div>
          <%= f.submit "Entrar" %>
        </div>
        <% if devise_mapping.rememberable? %>
          <div class="forget">
            <a href="/users/password/new">Esqueceu sua senha?</a>
          </div>
        <% end %>
      </form>
      <% end %>
      <!-- /Form -->      
    </div>
  </div>
  </div>
  <div class="clear"></div>
</div>
</div>
</div>
